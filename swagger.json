    "/device/{id}/image": {
      "get": {
        "tags": [
          "device"
        ],
        "summary": "Proxies the loaded media image",
        "description": "Proxies the image from the device's image url. Useful if your application can only access local content",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
